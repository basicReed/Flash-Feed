{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": ["post_id", "user_id", "content"],
  "properties": {
    "post_id": {
      "type": "integer",
      "default": 0,
      "title": "The post_id Schema",
      "examples": [123]
    },
    "user_id": {
      "type": "integer",
      "default": 0,
      "title": "The user_id Schema",
      "examples": [456]
    },
    "content": {
      "type": "string",
      "default": "",
      "title": "The content Schema",
      "examples": ["This is my comment"]
    }
  },
  "examples": [
    {
      "post_id": 123,
      "user_id": 456,
      "content": "This is my comment"
    }
  ]
}
