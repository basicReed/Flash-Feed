{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": ["userId", "postId", "txtContent"],
  "properties": {
    "userId": {
      "type": "integer",
      "default": 0,
      "title": "The userId Schema",
      "examples": [1]
    },
    "postId": {
      "type": "integer",
      "default": 0,
      "title": "The postId Schema",
      "examples": [4]
    },
    "txtContent": {
      "type": "string",
      "default": "",
      "title": "The txtContent Schema",
      "examples": ["Great post!"]
    }
  },
  "examples": [
    {
      "userId": 1,
      "postId": 4,
      "txtContent": "Great post!"
    }
  ]
}
