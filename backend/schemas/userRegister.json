{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "username",
    "password",
    "firstName",
    "lastName",
    "email",
    "imageUrl"
  ],
  "properties": {
    "username": {
      "type": "string",
      "default": "",
      "title": "The username Schema",
      "examples": ["johndoe"]
    },
    "password": {
      "type": "string",
      "default": "",
      "title": "The password Schema",
      "examples": ["password123"]
    },
    "firstName": {
      "type": "string",
      "default": "",
      "title": "The firstName Schema",
      "examples": ["John"]
    },
    "lastName": {
      "type": "string",
      "default": "",
      "title": "The lastName Schema",
      "examples": ["Doe"]
    },
    "email": {
      "type": "string",
      "default": "",
      "title": "The email Schema",
      "examples": ["johndoe@example.com"]
    },
    "imageUrl": {
      "type": "string",
      "default": "",
      "title": "The imageUrl Schema",
      "examples": ["https://example.com/profile_image.jpg"]
    }
  },
  "examples": [
    {
      "username": "johndoe",
      "password": "password123",
      "firstName": "John",
      "lastName": "Doe",
      "email": "johndoe@example.com",
      "imageUrl": "https://example.com/profile_image.jpg"
    }
  ]
}
