{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": ["username", "password"],
  "properties": {
    "username": {
      "type": "string",
      "default": "",
      "title": "The username Schema",
      "examples": ["johndoe"]
    },
    "password": {
      "type": "string",
      "default": "",
      "title": "The password Schema",
      "examples": ["password123"]
    }
  },
  "examples": [
    {
      "username": "johndoe",
      "password": "password123"
    }
  ]
}
