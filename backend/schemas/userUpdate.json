{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": ["username", "data"],
  "properties": {
    "username": {
      "type": "string",
      "default": "",
      "title": "The username Schema",
      "examples": ["johndoe"]
    },
    "data": {
      "type": "object",
      "default": {},
      "title": "The data Schema",
      "required": ["firstName", "lastName", "email", "imageUrl"],
      "properties": {
        "firstName": {
          "type": "string",
          "default": "",
          "title": "The firstName Schema",
          "examples": ["John"]
        },
        "lastName": {
          "type": "string",
          "default": "",
          "title": "The lastName Schema",
          "examples": ["Doe"]
        },
        "email": {
          "type": "string",
          "default": "",
          "title": "The email Schema",
          "examples": ["johndoe@example.com"]
        },
        "imageUrl": {
          "type": "string",
          "default": "",
          "title": "The imageUrl Schema",
          "examples": ["https://example.com/profile_image.jpg"]
        }
      },
      "examples": [
        {
          "firstName": "John",
          "lastName": "Doe",
          "email": "johndoe@example.com",
          "imageUrl": "https://example.com/profile_image.jpg"
        }
      ]
    }
  },
  "examples": [
    {
      "username": "johndoe",
      "data": {
        "firstName": "John",
        "lastName": "Doe",
        "email": "johndoe@example.com",
        "imageUrl": "https://example.com/profile_image.jpg"
      }
    }
  ]
}
